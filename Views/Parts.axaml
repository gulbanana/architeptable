<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:models="using:Architeptable.Models"
             x:Class="Architeptable.Views.Parts" x:DataType="models:Parts"
             mc:Ignorable="d" d:DesignWidth="300" d:DesignHeight="200" d:DataContext="{x:Static models:Mock.Parts}">
  <DataGrid Items="{Binding All}">
    <DataGrid.Columns>
      <DataGridTextColumn Header="Name" Binding="{Binding Name}" />
    </DataGrid.Columns>
  </DataGrid>
</UserControl>
